{
  "name": "graphql-boilerplate-frontend",
  "version": "1.0.0",
  "main": "src/index.jsx",
  "scripts": {
    "build": "rm -rf ./dist && ./node_modules/.bin/webpack -p",
    "start": "./node_modules/.bin/webpack-dev-server --hot --inline --mode development",
    "start:mock": "./node_modules/.bin/webpack-dev-server --hot --inline --mode development",
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "lint": "./node_modules/.bin/eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "./node_modules/.bin/eslint \"src/**/*.{ts,tsx}\" --fix",
    "stylelint": "./node_modules/.bin/stylelint 'src/**/*.{scss,css}'",
    "stylelint:fix": "./node_modules/.bin/stylelint 'src/**/*.{scss,css}' --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "apollo:generate": "apollo codegen:generate --target typescript --localSchemaFile=src/graphql/schema.graphql --excludes=node_modules/* --includes=./src/graphql/queries/*.{ts,tsx} --tagName=gql --outputFlat src/types --no-addTypename"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@graphql-codegen/typescript": "^1.3.1",
    "@storybook/react": "^5.1.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "apollo": "^2.15.0",
    "apollo-boost": "^0.4.3",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-schema": "^1.2.3",
    "apollo-storybook-react": "^0.2.1",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-inline-import-graphql-ast": "^2.4.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "es6-promise": "^4.2.8",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "graphql": "^14.3.1",
    "graphql-import": "^0.7.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "prettier-loader": "^2.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-apollo": "^2.5.8",
    "react-hot-loader": "^4.11.0",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest-mock-files.js",
      "^.+\\.(scss|css)$": "<rootDir>/jest-mock-styles.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.stories.*",
      "!src/types/**/*",
      "!node_modules/**"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "edge >= 38",
    "safari >= 9",
    "ie >= 11",
    "chrome >= 60",
    "firefox >= 51"
  ]
}
